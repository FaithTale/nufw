objectclass ( 18.3.6.1.1.18.2.1 NAME 'NuAccessControlList'
	DESC 'An IP Acl'
	MUST (cn $ SrcIPStart $ SrcIPEnd $ DstIPStart $ DstIPEnd $ Decision )
	MAY (Proto $ SrcPortStart $ SrcPortEnd $ DstPortStart $ DstPortEnd $ DstPort $ SrcPort  $ OsName $ OsVersion $ OsRelease $ AppName $ AppSig $ Group $ description  ) )

objectclass ( 18.3.6.1.1.18.2.2 NAME 'NuAccount' SUP top AUXILIARY
	DESC 'Abstraction of an account with Nufw attributes'
	MUST ( cn $ uid $ uidNumber $ Group  )
	MAY ( userPassword $ loginShell $ gecos $ description $ homeDirectory)  )
